<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence name="Pick_and_Place">
            <Action ID="MoveJoints" commands="0.1 -1.49998 -1.18997 1.94002 0.67 1.03001 -0.5" name="Move_To_Start_Position"/>
            <Sequence name="Pick_Block">
                <Action ID="MoveToPose" name="Move_To_Pick_Block_Hove_Pose" pose="0.7 0.158 0.021 -0.02496 0.99965 0.00738 0.00486"/>
                <Action ID="SetGripper" name="Open_Gripper" position="100.0"/>
                <Action ID="MoveToPose" name="Move_To_Pick_Block_Gripper_Pose" pose="0.7 0.158 -0.129 -0.02496 0.99965 0.00738 0.00486"/>
                <Action ID="SetGripper" name="Close_Gripper" position="70.0"/>
                <Action ID="MoveToPose" name="Move_To_Picked_Block_Hove_Pose" pose="0.7 0.158 -0.021 -0.02496 0.99965 0.00738 0.00486"/>
            </Sequence>
            <Sequence name="Place_Block">
                <Action ID="MoveToPose" name="Move_To_Place_Block_Hove_Pose" pose="0.75 -0.05 -0.021 -0.02496 0.99965 0.00738 0.00486"/>
                <Action ID="MoveToPose" name="Move_To_Place_Block_Grip_Pose" pose="0.75 -0.05 -0.129 -0.02496 0.99965 0.00738 0.00486"/>
                <Action ID="SetGripper" name="Open_Gripper" position="100.0"/>
                <Action ID="MoveToPose" name="Move_To_Relased_Block_Hove_Pose" pose="0.75 -0.05 -0.021 -0.02496 0.99965 0.00738 0.00486"/>
            </Sequence>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Action ID="MoveJoints">
            <input_port default="0 0 0 0 0 0 0" name="commands">position of each joint</input_port>
        </Action>
        <Action ID="MoveToPose">
            <input_port default="0 0 0 0 0 0 1" name="pose"/>
        </Action>
        <Action ID="SetGripper">
            <input_port default="0.0" name="position">position of the gripper</input_port>
        </Action>
    </TreeNodesModel>
    <!-- ////////// -->
</root>

